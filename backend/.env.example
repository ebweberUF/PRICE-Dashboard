# PRICE Dashboard Backend Configuration
# Copy this file to .env and fill in the values

# Environment
NODE_ENV=development

# Server
PORT=3001

# Database (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=price_app
DB_PASSWORD=your_password_here
DB_DATABASE=price_production
DB_SSL=false

# Session
SESSION_SECRET=your_session_secret_here

# Shibboleth SSO (production only)
SHIBBOLETH_ENABLED=false

# API Integrations (optional during development)
# REDCAP_API_URL=
# REDCAP_API_TOKEN=
# ELAB_API_URL=
# ELAB_API_KEY=
# SHAREPOINT_CLIENT_ID=
# SHAREPOINT_CLIENT_SECRET=
# XNAT_URL=
# XNAT_USERNAME=
# XNAT_PASSWORD=
